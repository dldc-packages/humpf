{
  "name": "humpf",
  "version": "3.0.1",
  "description": "Damped Spring position as a function of time",
  "keywords": [
    "spring",
    "damped",
    "damped-spring",
    "simulation",
    "for-t",
    "time",
    "decay",
    "smooth"
  ],
  "homepage": "https://github.com/etienne-dldc/humpf#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/humpf/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etienne-dldc/humpf.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc <e.deladonchamps@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/humpf.es.js",
      "require": "./dist/humpf.cjs.js"
    }
  },
  "main": "./dist/humpf.cjs.js",
  "module": "./dist/humpf.es.js",
  "types": "./dist/mod.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && vite build && yarn build:types && denoify",
    "build:types": "tsc -p ./tsconfig.build.json",
    "codecov": "codecov",
    "example:dev": "vite example",
    "example:run": "vite preview example",
    "lint": "prettier . --check && eslint . && tsc --noEmit",
    "lint:fix": "prettier . --write . && eslint . --fix",
    "release": "yarn build && np",
    "test": "yarn lint && jest --coverage",
    "test:run": "jest",
    "test:clear-cache": "jest --clearCache",
    "test:clear-cache:run": "jest --clearCache && jest",
    "test:watch": "jest --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/react": "^17.0.26",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "codecov": "^3.8.3",
    "denoify": "^0.10.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "framesync": "^6.0.0",
    "jest": "^27.2.4",
    "jest-ts-tsx-resolver": "^1.0.0",
    "np": "^7.5.0",
    "prettier": "^2.4.1",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "vite": "^2.6.2"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
