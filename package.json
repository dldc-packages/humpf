{
  "name": "humpf",
  "version": "4.0.0-5",
  "description": "Damped Spring position as a function of time",
  "keywords": [
    "spring",
    "damped",
    "damped-spring",
    "simulation",
    "for-t",
    "time",
    "decay",
    "smooth"
  ],
  "homepage": "https://github.com/etienne-dldc/humpf#readme",
  "bugs": {
    "url": "https://github.com/etienne-dldc/humpf/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etienne-dldc/humpf.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc <e.deladonchamps@gmail.com>",
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/mod.mjs",
      "require": "./dist/mod.js"
    }
  },
  "main": "./dist/mod.js",
  "module": "./dist/mod.mjs",
  "types": "./dist/mod.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsup src/mod.ts --format esm,cjs --dts && denoify",
    "dev": "tsup src/mod.ts --format esm,cjs --watch",
    "lint": "prettier src --check && eslint src && tsc --noEmit",
    "lint:fix": "prettier src --write && eslint src --fix",
    "release": "yarn run build && np",
    "test": "yarn run lint && jest --coverage",
    "test:run": "jest",
    "test:watch": "jest --watch",
    "test:cov:watch": "jest --coverage --watch --all",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "devDependencies": {
    "@types/jest": "^27.5.0",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "canvas": "^2.9.1",
    "denoify": "^0.11.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "jest": "^28.1.0",
    "np": "^7.6.1",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.2",
    "tslib": "^2.3.1",
    "tsup": "^5.12.7",
    "typescript": "^4.6.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
